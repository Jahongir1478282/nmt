{
  "manifest_version": 3,
  "name": "Antivirus",
  "version": "2.0",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["questions.json"],
      "matches": ["<all_urls>"]
    }
  ]
}